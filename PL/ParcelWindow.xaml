<Window x:Class="PL.ParcelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="ParcelWindow" Height="450" Width="800">
    <Grid x:Name="GridMainParcel">
        <Button x:Name="close" Content="x" HorizontalAlignment="Right" Margin="750,4,0,0" VerticalAlignment="Top" Height="29" Width="29" Background="#FFF31414" FontSize="22" FontWeight="Bold" FontFamily="Arial Black"/>
        <Grid  x:Name="GridAddParcel">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="211*"/>
                <ColumnDefinition Width="189*"/>
                <ColumnDefinition Width="200*"/>
                <ColumnDefinition Width="200*"/>
            </Grid.ColumnDefinitions>



        </Grid>
        <Grid  x:Name="GridUpParcel" d:IsHidden="True">
            <Grid.RowDefinitions>
                <RowDefinition Height="93*"/>
                <RowDefinition Height="115*"/>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="71*"/>
                <ColumnDefinition Width="89*"/>
            </Grid.ColumnDefinitions>


        </Grid>


    </Grid>
</Window>
