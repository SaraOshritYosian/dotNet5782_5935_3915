<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="680" Width="800">
    <Grid>
        <Grid>

            <Grid.Background>
                <ImageBrush/>
            </Grid.Background>
            <Image HorizontalAlignment="Center" Height="680" VerticalAlignment="Top" Width="800" Source="/drone-delivery-supply-chain-1200px-1024x690.jpg" RenderTransformOrigin="0.5,0.5" Margin="0,0,0,0" Stretch="Fill"/>
            <Button Content="Enter" HorizontalAlignment="Center" Margin="0,500,0,0" VerticalAlignment="Top" Height="67" Width="171" Click="Button_Click" FontFamily="Cooper Black" FontSize="48">
                <Button.Background>
                    <ImageBrush/>
                </Button.Background>
            </Button>

        </Grid>


    </Grid>
</Window>
